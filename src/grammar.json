{
  "name": "typst",
  "rules": {
    "markup_block": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "line"
          },
          {
            "type": "SYMBOL",
            "name": "_emptyline"
          }
        ]
      }
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "heading"
            },
            {
              "type": "SYMBOL",
              "name": "_line_content"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_emptyline": {
      "type": "SYMBOL",
      "name": "_newline"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n|\\r\\n?"
    },
    "heading": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_heading_prefix"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_line_content"
          },
          "named": true,
          "value": "heading_text"
        }
      ]
    },
    "_heading_prefix": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 10,
        "content": {
          "type": "PATTERN",
          "value": "=+[ |\\t]+"
        }
      }
    },
    "_line_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "strong"
          },
          {
            "type": "SYMBOL",
            "name": "_text"
          }
        ]
      }
    },
    "_text": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9]+"
        },
        {
          "type": "PATTERN",
          "value": "[^a-zA-Z0-9]"
        }
      ]
    },
    "strong": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_strong_delim_opn"
        },
        {
          "type": "SYMBOL",
          "name": "_line_content"
        },
        {
          "type": "SYMBOL",
          "name": "_strong_delim_cls"
        }
      ]
    },
    "_strong_delim_opn": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 5,
        "content": {
          "type": "PATTERN",
          "value": "[^a-zA-Z0-9]\\*"
        }
      }
    },
    "_strong_delim_cls": {
      "type": "PREC",
      "value": 5,
      "content": {
        "type": "PATTERN",
        "value": "\\*[^a-zA-Z0-9]"
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_comment"
        }
      ]
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 20,
        "content": {
          "type": "PATTERN",
          "value": "\\/\\/.*"
        }
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

